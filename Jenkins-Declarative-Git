pipeline{
    agent any
    stages{
       stage('Git checkout'){
            steps{
              git branch: 'main', credentialsId: 'kavya', url: 'https://github.com/kavyasrini24/javawebapp.git'
            }
         } 
          stage('Build'){
            steps{
                sh 'mvn clean package'
            }
        }
    }
}
